{
  "manifest_version": 2,
  "name": "Stash",
  "version": "1.0",
  "author": "Arthur Hirata",
  "description": "Stash tabs",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage", "tabs"]
}
