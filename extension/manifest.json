{
  "manifest_version": 2,
  "name": "Stash",
  "version": "1.0",
  "author": "Arthur Hirata",
  "description": "Stash tabs",
  "browser_action": {
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "permissions": ["storage", "tabs"],
  "commands": {
    "stash": {
      "description": "Stash tabs",
      "suggested_key": {
        "default": "Ctrl+Shift+S"
      }
    },
    "apply_stash": {
      "description": "Apply stashed tabs",
      "suggested_key": {
        "default": "Ctrl+Shift+A"
      }
    }
  }
}
